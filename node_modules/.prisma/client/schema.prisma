generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model audit_log {
  id                        Int       @id @default(autoincrement())
  action_name               String    @unique
  note                      String? 
  created_at                DateTime?
  created_by                Int?
}
